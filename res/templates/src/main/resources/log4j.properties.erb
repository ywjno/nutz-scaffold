log4j.rootLogger=debug, <%= java_style_project_name %>
log4j.category.org.nutz=debug, <%= java_style_project_name %>

log4j.additivity.org.nutz=false

#Appenders ...
log4j.appender.<%= java_style_project_name %>=org.apache.log4j.ConsoleAppender
log4j.appender.<%= java_style_project_name %>.layout=org.apache.log4j.PatternLayout
log4j.appender.<%= java_style_project_name %>.layout.ConversionPattern=%d [%t] %-5p %c - %m%n
